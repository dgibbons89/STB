<div>
  <% if user_signed_in? %>
    You have <%= pluralize(current_user.friends_count, "friend") %> on Facebook!
  <% end %>
</div>

<% if current_user %>
  <div>
    <table>
      <tr>
              </tr>
      <% current_user.friends_birthday.each do |d| %>
     
          <tr>
            <td><%= d["name"] %></td>
            <td><%= d["birthday"] %></td>
          </tr>
        <% end  %>
      <% end %>
      
    </table>
  </div>
  